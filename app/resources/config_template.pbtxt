# https://github.com/triton-inference-server/common/blob/main/protobuf/model_config.proto
name: "{{model_name}}"
backend: "python"
max_batch_size: 0
input [
  {
    name: "image"
    data_type: TYPE_UINT8
    dims: [ -1, -1, 3 ]
  }
]
output [
  {
    name: "score"
    data_type: TYPE_FP32
    dims: [ 1 ]
  },
  {
    name: "confidence"
    data_type: TYPE_FP32
    dims: [ 1 ]
  },
  {
    name: "probability"
    data_type: TYPE_FP32
    dims: [ 1 ]
  },
  {
    name: "label"
    data_type: TYPE_BOOL
    dims: [ 1 ]
  }
]
model_warmup: [
  {
    name: "warmup_with_zero_image"
    batch_size: 1
    inputs {
      key: "image"
      value {
        dims: [ 512, 300, 3 ]
        data_type: TYPE_UINT8
        zero_data: true
      }
    }
  }
]